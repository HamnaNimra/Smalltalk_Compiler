    selector notNil ifTrue: [
        SourceManager current sendersOf: selector asSymbol ifNone: [
            selector := self findWildcardSelector: selector, '*'.
            selector isNil ifTrue: [^self].
            SourceManager current sendersOf: selector asSymbol]]!    ! 
