    self icon: self icon.

    self isHandleOk ifFalse: [ self halt: 'Unable to create window...'. ^nil ].

    self systemMenuHandle.

    "menu and subpanes created in the wmCreate message process"

    "Set the size of the frame window."
    self handle
        setWindowPos: nil
        rectangle: myRealRectangle
        fs: SwpNozorder.

